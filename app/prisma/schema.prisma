generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  PARTICIPANT
  ADMIN
}


model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  role      UserRole @default(PARTICIPANT)
}

